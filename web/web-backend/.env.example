# Copy this file to .env and fill in your actual values

# Database Configuration
# For local PostgreSQL
# DATABASE_URL=postgresql://username:password@localhost:5432/mydb

# For Supabase PostgreSQL
DATABASE_URL=postgresql://postgres.your-project-id:your-password@aws-region.pooler.supabase.com:5432/postgres

# Opik Configuration (optional - for LLM tracing)
OPIK_API_KEY=your-opik-api-key
OPIK_PROJECT_NAME=FretCoach
OPIK_WORKSPACE=your-opik-workspace
OPIK_URL_OVERRIDE=https://www.comet.com/opik/api

# Supabase (if using Supabase instead of direct PostgreSQL)
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your-anon-key

# Model Configuration
USE_OPENAI_MODEL=false
OPENAI_MODEL=gpt-4o-mini
GEMINI_MODEL=gemini-3-flash-preview

# API Keys
GOOGLE_API_KEY=your-google-api-key
OPENAI_API_KEY=your-openai-api-key

# For Minimax Anthropic integration
ANTHROPIC_BASE_URL=https://api.minimax.io/anthropic
ANTHROPIC_API_KEY=your-anthropic-api-key

# PostgreSQL Component Configuration (for direct connection)
DB_USER=postgres.your-project-id
DB_PASSWORD=your-supabase-db-password
DB_HOST=aws-region.pooler.supabase.com
DB_PORT=5432
DB_NAME=postgres
