# Copy this file to .env and fill in your actual values

# Tuya Smart Bulb Credentials
HAVELLS_ACCESS_ID=your-havells-access-id
HAVELLS_ACCESS_SECRET=your-havells-access-secret
HAVELLS_DEVICE_ID=your-havells-device-id
HAVELLS_REGION=in

# Required for research agents with external search
TAVILY_API_KEY=your-tavily-api-key

# Required for model usage
OPENAI_API_KEY=your-openai-api-key
GROQ_API_KEY=your-groq-api-key
GOOGLE_API_KEY=your-google-api-key
OPENROUTER_API_KEY=your-openrouter-api-key
PPLX_API_KEY=your-perplexity-api-key

# For Minimax Anthropic integration
ANTHROPIC_BASE_URL=https://api.minimax.io/anthropic
ANTHROPIC_API_KEY=your-anthropic-api-key

# Opik Configuration (for LLM tracing)
OPIK_API_KEY=your-opik-api-key
OPIK_WORKSPACE=your-opik-workspace
OPIK_PROJECT_NAME=FretCoach

# Optional if you are using Opik Cloud:
OPIK_URL_OVERRIDE=https://www.comet.com/opik/api

# PostgreSQL Local configuration
# DB_USER=your-username
# DB_PASSWORD=your-password
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=mydb

# PostgreSQL Supabase configuration
DB_USER=postgres
DB_PASSWORD=your-supabase-db-password
DB_HOST=your-supabase-host.supabase.co
DB_PORT=5432
DB_NAME=postgres

# Deployment Type
DEPLOYMENT_TYPE=fretcoach-studio
# To switch to portable deployment, uncomment the following line:
# DEPLOYMENT_TYPE=fretcoach-portable

# Debug Configuration
FRETCOACH_DEBUG_AUDIO=1
